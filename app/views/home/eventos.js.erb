<%if @filter  %>
    $("#paginate_itens").empty();
<%end%>
    $("#paginate_itens").append(" <%= j render "item", itens: @eventos%> ");

<% if @eventos.next_page %>
  $('.pagination').replaceWith('<%= j will_paginate @eventos, :remote => true %>');
<% else %>
  $(window).off('scroll');
  $('.pagination').remove();
<% end %>
    