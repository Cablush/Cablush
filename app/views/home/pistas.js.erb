<%if @filter  %>
    $("#paginate_itens").empty();
<%end%>
    $("#paginate_itens").append(" <%= j render "item", itens: @pistas%> ");

<% if @pistas.next_page %>
  $('.pagination').replaceWith('<%= j will_paginate @pistas, :remote => true %>');
<% else %>
  $(window).off('scroll');
  $('.pagination').remove();
<% end %>
    