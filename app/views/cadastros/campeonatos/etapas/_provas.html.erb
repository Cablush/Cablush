<ul>
  <% etapa.provas.each do |prova| %>
    <li>
      <div class="elem-list">
        <div class="title_head_list">
          <%= t('views.cadastros.prova_nome', prova: prova.id) %>

          <p class="tip">
            <%= t('views.cadastros.prova_tip',
                  current: prova.participantes.length,
                  max: etapa.max_competidores_prova) %>
          </p>
        </div>
        <div class="participants">
          <%= render 'participantes', etapa: etapa, prova: prova %>
        </div>
      </div>
    </li>
  <%end%>
</ul>