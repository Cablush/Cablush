<div class="content-box">
    <div class="wrap">
        <ul class="events">
            <li><%= link_to 'Cadastros', cadastros_path %></li>
            <li class="active"><a href="#">Pistas</a></li>
            <li><%= link_to 'Nova Pista', cadastros_new_pista_path %></li>
        </ul>
        <div class="clear"></div>
    </div>
</div>
<div class="main">
    <div class="wrap">
        <ul class="list1">
            <% @pistas.each do |pista| %>
              <li>
                  <%= link_to pista.nome, cadastros_edit_pista_path(pista) %>
              </li>
            <% end %>
        </ul>        
    </div>
    <div class="clear"></div>
</div>